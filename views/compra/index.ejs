<div>
	<form method="get" action="/generar" >      


			<h2 for="resp">Precio de la compra: <%= precio %> €</h2>

			 <br />
			 <div class=centraTabla>
			 <table class="w3-table-all" >
			 <tr>
					        <th> <strong> PRODUCTO </strong> </th>
					        <th><strong>CANTIDAD</strong></th>
					        <th><strong>PRECIO UNIDAD</strong></th>
					        <th><strong>TOTAL</strong></th>
			 </tr>
			 		<!-- con eso recorro el json y en el_buen_producto meto el json con mis propiedades -->
				<%  Object.keys(productos_totales).map(function (el_buen_producto){ %>
					
					
					   
					     <tr>
					 		<td><%= productos_totales[el_buen_producto].nombre %></td>
					 		<td>  <%= productos_totales[el_buen_producto].cantidad %></td>
					 		<td>  <%= productos_totales[el_buen_producto].precio %> €</td>
					 		<td>  <%= productos_totales[el_buen_producto].precioTotal %> €</td>
							
					</tr>
			 	<% 	 } ) %>
			 	</table>
					</div>
			 	<br/>
			 	<p>Puede modificar el precio a cobrar en el siguiente espacio</p>
        <input type="text" id="precio" name="precio" value= "<%= precio %>" size="30" autocomplete="off" style="text-align:center" />
			<br/>

        	  <input type="hidden" name="url" value=<%= url %> />
        	  <br>
        	  <div class="w3-row-padding">
			    <div class="w3-bar">
			        <input class="w3-button w3-blue" type="submit" value="Generar">
			    </div>
			</div>
			<br>
			<br>
			<a href="/productos"><button type="button">Cancelar</button></a>
			

</div>


